  `referencia` varchar(156) default NULL,
  `fechaini` date default NULL,
  `fechafin` date default NULL,
  `dvestado` varchar(156) default NULL,
  `activo` varchar(3) default NULL,
  `proyecto` int(11) default NULL,
  `presupuesto` float(11,2) default NULL,
